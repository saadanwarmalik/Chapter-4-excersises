#include <iostream>
#include <vector>
using namespace std;

int main()
{
	vector<int> amount(5);
	cout<<"enter five numbers"<<endl;
	
	for{int i=0; i<5; i++;}
		{
			cin>>amount[i];
		}
		
    cout<<"these are the numbers you entered:"<<endl;
		
	for {int f=0; f<5; f++;}
		{
			
			cout<<amount[f];
		}
}
			